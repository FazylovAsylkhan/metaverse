    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "airbnb": "^0.0.2",
    "babel-loader": "^9.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.26.0",
